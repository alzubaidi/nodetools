{
  "name": "nodetools",
  "version": "1.1.5",
  "description": "Node.js based tools package.",
  "author": "Tareq Al-Zubaidi <tareqzubidee@gmail.com>",
  "main": "./lib/tools.js",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "test": "mocha --require babel-register test/*.spec.js",
    "build": "babel src --out-dir lib",
    "build:watch": "babel src --watch --out-dir lib"
  },
  "directories": {
    "test": "./test",
    "lib": "./lib",
    "src": "./src"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.5"
  },
  "engines": {
    "node": ">=4.4.4"
  },
  "repository": "alzubaidi/nodetools",
  "keywords": [
    "node.js",
    "tools",
    "md5",
    "sha1",
    "sha256",
    "sha512",
    "base64 encode",
    "base64 decode",
    "url decode",
    "url encode",
    "isset",
    "is numeric",
    "mime-type",
    "mime",
    "hash",
    "encode",
    "decode"
  ],
  "bugs": {
    "url": "https://github.com/alzubaidi/nodetools/issues"
  },
  "license": "MIT",
  "dependencies": {
    "mime-types": "^2.1.12"
  }
}
